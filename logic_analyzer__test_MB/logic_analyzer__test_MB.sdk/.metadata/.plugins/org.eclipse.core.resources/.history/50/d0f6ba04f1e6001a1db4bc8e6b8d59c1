#include "xparameters.h"
#include "hi_speed_sampler.h"
#include "xgpio.h"
#include <stdbool.h>

XPAR_HI_SPEED_SAMPLER_0_DEVICE_ID;
XPAR_HI_SPEED_SAMPLER_0_HI_SPEED_SAMPLER_BASEADDR;
PAR_HI_SPEED_SAMPLER_0_HI_SPEED_SAMPLER_HIGHADDR;

bool Sampler_IsFullyWritten(void);
uint32_t Sampler_Read();
bool Sampler_Init;
void Sampler_Enable();
void SamplerDisable();

XGpio gpio_handle;

int main(){
	uint32_t value = 0;
	if (XST_SUCCESS == XGpio_Initialize(&gpio_handle,XPAR_GPIO_0_DEVICE_ID)){
		goto error;
	}
	XGpio_SetDataDirection(&gpio_handle,0,0x000); // set as output
	XGpio_DiscreteWrite(&gpio_handle,0,value);

if(Sampler_IsFullyWritten()){
	value = Sampler_Read();
}




error: while(1);
}
